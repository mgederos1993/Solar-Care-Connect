{
  "presets": [
    [
      "babel-preset-expo",
      {
        "web": {
          "useTransformReactJSXExperimental": false
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@": "./"
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "babel-plugin-transform-remove-console",
          {
            "exclude": ["error", "warn"]
          }
        ]
      ]
    },
    "web": {
      "presets": [
        [
          "babel-preset-expo",
          {
            "web": {
              "useTransformReactJSXExperimental": false,
              "disableImportExportTransform": false
            }
          }
        ]
      ]
    }
  }
}