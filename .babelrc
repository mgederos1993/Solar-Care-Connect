{
  "presets": ["babel-preset-expo"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@": "./"
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "babel-plugin-transform-remove-console",
          {
            "exclude": ["error", "warn"]
          }
        ]
      ]
    }
  }
}